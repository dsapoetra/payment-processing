  "rewrites": [
    {
      "source": "/api/v1/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/health",
      "destination": "/api/index"
    },
    {
      "source": "/swagger/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/ui/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/admin",
      "destination": "/api/index"
    }
  ]
}
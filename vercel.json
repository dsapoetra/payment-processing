{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/health",
      "dest": "/api/index.js"
    },
    {
      "src": "/swagger/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/ui/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/admin",
      "dest": "/api/index.js"
    }
  ]
}